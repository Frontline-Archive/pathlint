    "eslint": "eslint",
    "jscs": "jscs",
    "pathlint": "pathlint"
  },
  "dependencies": {
    "command-line-args": "^1.0.0",
    "glob": "^5.0.14"
  },
  "devDependencies": {
    "eslint": "0.23.0",
    "gulp": "^3.9.0",
    "gulp-istanbul": "^0.10.0",
    "gulp-mocha": "^2.1.3",
    "jscs": "1.12.0",
    "prettyjson": "^1.1.3",
    "proxyquire": "^1.7.0",
    "should": "^7.0.4",
    "sinet-coding-conventions": "^1.31.3"
  },
  "bin": {
    "pathlint": "bin/pathlint"
  }
